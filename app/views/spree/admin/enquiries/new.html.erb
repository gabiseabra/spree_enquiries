<% content_for :page_title do %>
  <%= link_to plural_resource_name(Spree::Enquiry),
              spree.admin_enquiries_url %> /
  <%= Spree.t(:new_enquiry) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @enquiry } %>

<%= form_for [:admin, @enquiry] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>